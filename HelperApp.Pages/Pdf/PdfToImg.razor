@page "/pdf-to-img"

<div class="pdf-title">
    pdf转换图片
</div>
<MDivider></MDivider>

<MFileInput
    @bind-Value="_files"
    Placeholder=""
    Label="上传文件"
    Multiple
    Accept=""
    PrependIcon="mdi-paperclip">
    <SelectionContent Context="context">
        <MChip
            Small
            Label
            Color="primary">
            @_files[context.index].Name
        </MChip>
    </SelectionContent>
</MFileInput>


<div class="pdf-div">
    <button @onclick="PdfToImgAsync" class="pdf-but">
        转换
    </button>
</div>
